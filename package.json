{
  "name": "7daystodie-api-wrapper",
  "version": "1.5.1",
  "description": "Interact with a 7DTD web API from Allocs Fixes",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "engines": {
    "node": ">=8.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatalysmsServerManager/7-Days-to-Die-API-wrapper"
  },
  "dependencies": {
    "@types/qs": "^6.9.1",
    "node-fetch": "^2.6.0",
    "qs": "^6.9.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^7.0.0",
    "@types/node": "^13.13.2",
    "@types/node-fetch": "^2.5.2",
    "@types/snekfetch": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.0.0",
    "shx": "^0.3.0",
    "ts-node": "^8.3.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx --ignore-path ./.gitignore .",
    "test": "mocha test",
    "build": "tsc -D",
    "build-watch": "tsc --watch",
    "docs": "typedoc --out docs/ lib/ --theme minimal --name '7 Days to Die API wrapper' --includeDeclarations --excludeExternals --exclude lib/getStats.ts",
    "postdocs": "shx cp .nojekyll docs/.nojekyll",
    "precover": "npm run build",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha dist/test/*.js -- -R spec"
  },
  "mocha": {
    "reporter": "spec",
    "extension": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [
    "web",
    "api",
    "7",
    "days",
    "to",
    "die",
    "alloc",
    "fixes"
  ],
  "author": "Catalysm",
  "license": "GPL-3.0"
}